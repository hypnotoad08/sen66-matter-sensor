CONFIG_IDF_TARGET="esp32s3"

# â”€â”€â”€â”€â”€ Flash Configuration â”€â”€â”€â”€â”€
CONFIG_ESPTOOLPY_FLASHMODE_QIO=y
CONFIG_ESPTOOLPY_FLASHSIZE_16MB=y

# â”€â”€â”€â”€â”€ PSRAM (8MB OPI) â”€â”€â”€â”€â”€
CONFIG_SPIRAM=y
CONFIG_SPIRAM_MODE_OCT=y
CONFIG_SPIRAM_SPEED_80M=y

# â”€â”€â”€â”€â”€ RTC Clock â”€â”€â”€â”€â”€
CONFIG_RTC_CLK_SRC_EXT_CRYS=y

# â”€â”€â”€â”€â”€ Console over UART â”€â”€â”€â”€â”€
CONFIG_ESP_CONSOLE_USB_SERIAL_JTAG=n
CONFIG_ESP_CONSOLE_UART=y
CONFIG_ESP_CONSOLE_UART_NUM=0
CONFIG_ESP_CONSOLE_UART_BAUDRATE=115200

# â”€â”€â”€â”€â”€ FreeRTOS Tick â”€â”€â”€â”€â”€
CONFIG_FREERTOS_HZ=1000

# â”€â”€â”€â”€â”€ UART Configuration (for debugging and optional RCP) â”€â”€â”€â”€â”€
CONFIG_UART_DRIVER_ENABLED=y
CONFIG_UART_NUM=1
CONFIG_UART_TX_GPIO=43
CONFIG_UART_RX_GPIO=44
CONFIG_UART_BAUD_RATE=115200
CONFIG_UART_DATA_BITS=8
CONFIG_UART_PARITY_DISABLE=y
CONFIG_UART_STOP_BITS_1=y
CONFIG_UART_HW_FLOWCTRL_DISABLE=y

# â”€â”€â”€â”€â”€ Matter Specific Settings â”€â”€â”€â”€â”€
CONFIG_ESP_MATTER_ENABLE=y
CONFIG_ESP_MATTER_LOG_LEVEL_INFO=y
CONFIG_ESP_MATTER_TRANSPORT_THREAD=y
CONFIG_ESP_MATTER_TRANSPORT_UART_ENABLE=y
CONFIG_ESP_MATTER_UART_PORT_NUM=1
CONFIG_ESP_MATTER_SERIAL_SPEED=115200
CONFIG_ESP_MATTER_TRANSPORT_BLE=y
CONFIG_ESP_MATTER_COMMISSIONER_ENABLE=y

# â”€â”€â”€â”€â”€ Network Endpoint IDs â”€â”€â”€â”€â”€
CONFIG_WIFI_NETWORK_ENDPOINT_ID=0
CONFIG_THREAD_NETWORK_ENDPOINT_ID=1

# â”€â”€â”€â”€â”€ Crypto â”€â”€â”€â”€â”€
CONFIG_LIBSODIUM_USE_MBEDTLS_SHA=y
CONFIG_MBEDTLS_HKDF_C=y

# â”€â”€â”€â”€â”€ mbedTLS for ECJPAKE and DTLS â”€â”€â”€â”€â”€
CONFIG_MBEDTLS_KEY_EXCHANGE_ECJPAKE=y
CONFIG_MBEDTLS_ECJPAKE_C=y
CONFIG_MBEDTLS_SSL_PROTO_DTLS=y

# â”€â”€â”€â”€â”€ Bluetooth - BLE/NimBLE â”€â”€â”€â”€â”€
CONFIG_BT_ENABLED=y
CONFIG_BT=y
CONFIG_BT_NIMBLE_ENABLED=y
CONFIG_BLUEDROID_ENABLED=n
CONFIG_BT_NIMBLE_HOST=y
CONFIG_BT_NIMBLE_ROLE_PERIPHERAL=y
CONFIG_BT_NIMBLE_ROLE_OBSERVER=y
CONFIG_BT_NIMBLE_ROLE_BROADCASTER=y
CONFIG_BT_NIMBLE_GATT_SERVER=y
CONFIG_BT_NIMBLE_HCI_EVT_BUF_SIZE=70

# â”€â”€â”€â”€â”€ OpenThread FTD Mode â”€â”€â”€â”€â”€
CONFIG_OPENTHREAD_ENABLED=n
CONFIG_OPENTHREAD_FTD=y
CONFIG_OPENTHREAD_SRP_CLIENT=y
CONFIG_OPENTHREAD_DNS_CLIENT=y
CONFIG_OPENTHREAD_LOG_LEVEL_DYNAMIC=n
CONFIG_OPENTHREAD_LOG_LEVEL_NOTE=y
CONFIG_OPENTHREAD_CLI=n

# â”€â”€â”€â”€â”€ LwIP Adjustments for Thread â”€â”€â”€â”€â”€
CONFIG_LWIP_IPV6_NUM_ADDRESSES=12
CONFIG_LWIP_MULTICAST_PING=y
CONFIG_LWIP_HOOK_IP6_ROUTE_DEFAULT=y
CONFIG_LWIP_HOOK_ND6_GET_GW_DEFAULT=y

# â”€â”€â”€â”€â”€ Discovery and Commissioning â”€â”€â”€â”€â”€
CONFIG_USE_MINIMAL_MDNS=n
CONFIG_ENABLE_EXTENDED_DISCOVERY=y
CONFIG_ENABLE_CHIP_SHELL=y

# â”€â”€â”€â”€â”€ Partition Table â”€â”€â”€â”€â”€
CONFIG_PARTITION_TABLE_CUSTOM=y
CONFIG_PARTITION_TABLE_FILENAME="partitions.csv"
CONFIG_PARTITION_TABLE_OFFSET=0x8000

# â”€â”€â”€â”€â”€ Disable Wi-Fi (Thread or BLE use) â”€â”€â”€â”€â”€
CONFIG_ESP_WIFI_ENABLED=y
CONFIG_ESP_WIFI_SOFTAP_SUPPORT=y

# â”€â”€â”€â”€â”€ Factory Data Providers â”€â”€â”€â”€â”€
# Disable the DS Peripheral support
CONFIG_ESP_SECURE_CERT_DS_PERIPHERAL=n


# Enable some options which reads CD and other basic info from the factory partition
CONFIG_ENABLE_ESP32_FACTORY_DATA_PROVIDER=y
CONFIG_ENABLE_ESP32_DEVICE_INSTANCE_INFO_PROVIDER=y
CONFIG_FACTORY_COMMISSIONABLE_DATA_PROVIDER=y
CONFIG_FACTORY_DEVICE_INSTANCE_INFO_PROVIDER=y
CONFIG_FACTORY_PARTITION_DAC_PROVIDER=y
CONFIG_CUSTOM_DEVICE_INSTANCE_INFO_PROVIDER=n


# Tune the mDNS stack size
CONFIG_MDNS_TASK_STACK_SIZE=8192

# Use PSRAM for logging buffers
CONFIG_LOG_TX_BUFFER_USE_PSRAM=y

# Limit NimBLE and Wi-Fi memory use
CONFIG_BT_NIMBLE_SVC_GATT_CLI_MAX=4
CONFIG_BT_NIMBLE_GAP_ROLE_CENTRAL=n
CONFIG_BT_NIMBLE_MAX_CONNECTIONS=1


